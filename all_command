 export https_proxy=http://192.168.0.108:7890   加入别人的代理
gedit ~/.bashrc               设置快捷键
 git clone -b xuzhefeng git@github.com:YellowOrz/moving_object_detection.git   //复制远程的分支
dpkg -i soft.version.deb    安装deb包


git push -u origin xuzhefeng:uphome --force   // 
git push origin feature-branch:feature-branch    //推送本地的feature-branch(冒号前面的)分支到远程origin的feature-branch(冒号后面的)分支(没有会自动创建)
首先在目录下git  clone  分支  然后在该分支下进行修改，最后使用上述命令进行传到远程分支
 
 
